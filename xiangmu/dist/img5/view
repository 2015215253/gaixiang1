<script type="text/javascript">
    $(function() {
        $(".gst-address-info").mouseenter(function () {
            $(this).addClass("on");
            $(".gst-address-list").show();
        }).mouseleave(function () {
            $(this).removeClass('on');
            $(".gst-address-list").hide();
            $(".gst-address-list li a").removeClass("gst-address-listSel");
            $('.gst-address-lower ul').html('');
        });

        //88滑动显示
        $('.receiptAddress').mouseover(function () {
            $('.pd-info9-down').show();
        }).mouseleave(function(){
            $('.pd-info9-down').hide();
        })
        //选择地址
        var province, city;
        $('.gst-address-list .provinces').on('click', function () {
            $('.gst-address-lower ul').html('');
            $(".gst-address-list li a").removeClass("gst-address-listSel");
            province = $(this).children('a').addClass('gst-address-listSel').text();
            lower = $(this).nextAll('.gst-address-lower').eq(0);
            lower.children('ul').html('<div class="gx-loading-sm"><span></span></div>');
            var html = '';
            $.ajax({
                url: 'http://www.g-emall.com/product/cities',
                method: 'get',
                dataType: 'json',
                data: {provinceId: $(this).attr('province-id')},
                success: function (data) {
                    cities = data.cities;
                    for (i = 0, len = cities.length; i < len; i++) {
                        city = cities[i];
                        html += '<li city-id="' + city.city_code + '"><a href="javascript:void(0)">' + city.city_name + '</a></li>';
                    }
                    lower.children('ul').html(html);
                }
            })

        });

        $('.gst-address-lower').delegate('li', 'click', function () {
            city = $(this).children('a').text();
            $('.gst-address-info').children('span').text(province + '  ' + city);
            freight($(this).attr('city-id'));
        });

        function freight(cityId) {
            $.ajax({
                url: 'http://www.g-emall.com/product/logistic',
                method: 'get',
                dataType: 'json',
                data: {
                    uId: '',
                    areaId: cityId,
                    properties: {
                        weight:0,
                        volume:0,
                        price:518.00000,
                        number: $('.pord-num').val()
                    }
                },
                success: function (data) {
                    var freights = data.freight;
                    len=freights.length;
                    if(len == 0){
                        $('.pd-info9-font1 p').text('包邮');
                        $('.pd-info9-down').remove();
                        return false;
                    }

                    var html = '';
                    for(i=0;i<len;i++){
                        html += "<p>"+ freights[i]['name'] + ":  " + freights[i]['price'].toFixed(2)  +"</p>";
                        if(i == 0){
                            $('.pd-info9-font1 p').text(freights[i]['name'] + ":  " + freights[i]['price'].toFixed(2));
                        }
                    }

                    $('.pd-info9-down').html(html);

                    if(data.sendAddressInfo){
                        $('.pd-info9-fr .sendAddress').html(data.sendAddressInfo+'&nbsp;&nbsp;&nbsp;至&nbsp;');
                    }
                }
            })
        }
        freight(237);
    })


    //购买、加入购物车确认是否有选择商品参数(i:需要选择的参数有几项)
    var ISPordshoppingCart="";
    var selLength; //选中规则个数
    var totalLength=$(".prod-property").length;//规格总数
    $(function(){
        var priceFormat = "￥0.00";
        var pricePrefix = priceFormat.slice(0, priceFormat.search(/\d/));
        var storeUid = 'a9e20485-d2f6-4b0d-b785-8a8cbc0ca97a';//商品所属店铺id
        if(''==1) {
            $.cookie('storeId', storeUid);
        }else{
            delCookie('login');delCookie('storeId');delCookie('owner');
        }
        //购物流程 --- 立即购买
        $('#pord-buy').click(function(){
            $(".shoppingCart-float").hide();
            if(!isSelectAll('pord-buy')) return false; //选择全部
            var storeID = '';
            if(storeUid == storeID || $.cookie('owner')=='true'){
                layer.alert("不能购买自家的商品", {
                    icon: 0,
                    title: '提示',
                    skin: 'layer-ext-gaiwang',
                    btn: ['确定']
                });
                return false;
            }
            if(!selectedStock) {
                showMsg('已经售完');
                return false;
            }
            var number = parseInt($('#pordParameter .pord-num').val());
            if(number <= 0 ){
                showMsg('购买数量必须大于0');
                return false;
            }
            if(catalogUniqueID == '0' || catalogUniqueID == '') {
                showMsg('商品此规格已经失效');
                return false;
            }
            if(!login()){
                $.cookie("action","buy");
                $.cookie("catalogUniqueID",catalogUniqueID);
                $.cookie("number",number);
                delCookie("product");
                return false;
            }

            $('#checkout-uid').val(catalogUniqueID);
            $('#checkout-num').val(number);
            $('#checkout-form').submit();
        });


        //加入购物车
        $("#pord-shoppingCart,#pord-shoppingCart-top").click(function(){
            $(".shoppingCart-float").hide();
            if(!isSelectAll('pord-shoppingCart')) return false;

            if(!selectedStock) {
                showMsg('已经售完');
                return false;
            }

            var number = parseInt($('#pordParameter .pord-num').val());
            if(number <= 0 ){
                showMsg('购买数量必须大于0');
                return false;
            }
            if(catalogUniqueID == '0' || catalogUniqueID == '') {
                showMsg('商品此规格已经失效');
                return false;
            }

            var product = {};
            var properties='',properties_value = '{';
            $('.prod-property .on').each(function(index){
                var o = $(this),p=o.parent();
                var spec = o.attr('data-value');
                properties = properties + o.attr('spec-id') + "[" + spec + "]";
                if(index == (selLength - 1)){
                    properties_value = properties_value + '"' + p.siblings('p').text().replace(/\：|\s\：/g,'') + '":"' + o.attr('spec-value') +'"';
                } else {
                    properties_value = properties_value + '"' + p.siblings('p').text().replace(/\：|\s\:/g,'') + '":"' + o.attr('spec-value') + '",';
                }
            })
            properties_value = properties_value + '}'
            product.properties = properties;
            product.propertiesValue = properties_value;
            product.quantity = number;
            product.goodsId = '5aaafcf4-1e20-4aba-81eb-b69a9f87beef';
            product.storeId = 'a9e20485-d2f6-4b0d-b785-8a8cbc0ca97a';
            product.catalogId = catalogUniqueID;
            product.price = price;
            product.shareUser = '';
            product._csrf = 'M1JGaF8wV1dYDSIbNFkxA30oBz8rezgBeQE/MStZHDVrOnQbER01Zw==';
            if(!login()) {
                $.cookie("action","cart");
                $.cookie("product",JSON.stringify(product));
                delCookie("catalogUniqueID");
                delCookie("number");
                return false;
            }else{
                var index = layer.load({icon:0});
                addCart(product,index);
            }
        });

        //选择商品参数
        if('1'){
            var spec = JSON.parse('[{"uniqueid":"81b53f18-e156-4be7-a6e6-fcd3d672b200","variant":"","variantName":"","quantityStock":"145","price":"518.00000","picture":"files/2015/07/01/740f97f6491226fa985762518fefb3eb.jpg"}]'); //规格选择
            var selectedStock = 145; //catalog库存
            var catalogUniqueID,catalog,price;
            if(spec.length == 1) {
                catalog = spec.pop();
                catalogUniqueID = catalog.uniqueid;//默认条目
                price = 518.00000;
            } else if(spec.length != 1 && 1){
                for(i in spec){
                    sp = spec[i];
                }
                if(parseInt(sp.quantityStock)){
                    $(".prod-property").addClass('selected');
                    $(".prod-property li").addClass('on');
                    catalogUniqueID = sp.uniqueid;
                    price = sp.price;
                }
            }
        }

        var $img = new Image();
        $img.src = $("#preview .jqzoom img").attr('jqimg');
        sImg($img);
        function sImg(obj){
            obj.onload = function(){
                if(obj.width<400){
                    $("#preview").addClass("s-img");
                }else{
                    $("#preview").removeClass("s-img");
                }
            };
        }

        $(".prod-property li").on('click',function(){
            var t = $(this);
            //选中切换
            if(t.hasClass("not-itme")) return false;
            if(!t.hasClass("not-itme") && !t.hasClass('on')) {
                t.siblings().removeClass("on");
                t.addClass("on").parent().addClass('selected');
                var l_src=t.find("img").attr("bimg");
                if(l_src != undefined ) {
                    $("#preview .jqzoom img").attr("src",l_src);
                    $("#preview .jqzoom img").attr("jqimg",l_src);
                    $img.src = t.find("img").attr("jqimg");
                    sImg($img);
                }
            } else {
                t.removeClass('on').parent().removeClass('selected');
                catalogUniqueID = '';
            }

            selLength = $(".prod-property").find(".on").length; // 已选中规格数
            if((totalLength - selLength) > 1){
                $('.prod-property li').removeClass('not-itme');
                return false;
            }
            //未选择大于1 不做处理
            checkStock(t.attr('spec-id'),$(".prod-property"));

        });

        /* 检查库存等消息 */
        function checkStock(self,pro)
        {
            var temp = key = new Array(0); //规格种类数

            var p,t,us,pn; // p是property，t是规格值
            var unselect = [] , selected = [];
            pro.each(function(){
                p = $(this);
                if(p.hasClass('selected')){
                    t = p.find('.on');
                    key[t.attr('spec-id')] = t.attr('data-value');
                } else {
                    us = p; //缓存未选中规格
                    p.children('li').each(function(eq){
                        pn = $(this).attr('spec-id');
                        unselect[eq] = $(this).attr('data-value');
                    })
                }
            });
            if(unselect.length){ //有值说明是未全选
                for(i in unselect){
                    key[pn] = unselect[i];
                    for(j in key){
                        selected.unshift(key[j]);
                    }
                    index = selected.sort(sortNumber).toString();
                    sp = spec[index];
                    if(sp.quantityStock <= 0) us.children("li").eq(i).addClass('not-itme');
                    else us.children("li").eq(i).removeClass('not-itme')
                    selected = [];

                }
                $('.pord-stockNum').text('库存' + 145 + '件');
                $('.size-dd .pord-num').attr('total',"145");

            } else { //反之全选
                for(i in key){
                    selected.unshift(key[i]);
                }
                index = selected.sort(sortNumber).toString();
                sp = spec[index];
                $('.pd-info5').html('<span>'+pricePrefix+'</span>'+ parseFloat(sp.hkPrice).toFixed(2));
                $('.pd-info12-box span').text((sp.hkPrice * 100 / 100).toFixed(2));
                if(parseInt(sp.quantityStock))
                    $('.pord-stockNum').text('库存'+sp.quantityStock+'件');
                else
                    $('.pord-stockNum').text('已售完');
                $('.pord-num').attr('total',sp.quantityStock).val(1);
                catalogUniqueID = sp.uniqueid;
                price = sp.price;
                selected = [];
                ///遍历所有规格，找出不肯选项
                temp = key;
                pro.each(function(){
                    li = $(this).children('li');
                    ch = li.eq(0).attr('spec-id');
                    if(li.length != 1 && ch != self ){
                        key = temp;
                        li.each(function(){
                            key[ch] = $(this).attr('data-value');
                            for(i in key) {
                                selected.unshift(key[i]);
                            }
                            index = selected.sort(sortNumber).toString();
                            sp = spec[index];
                            if (sp.quantityStock <= 0 && !$(this).hasClass('on')) $(this).addClass('not-itme');
                            else $(this).removeClass('not-itme');
                            selected = [];
                        })
                    }
                })
            }
        }

        //提示信息
        function showMsg(content){
            $('.size-msg').text(content).show().fadeOut(1500);
        }

        //是否全选规格了
        function isSelectAll(flag)
        {
            selLength = $(".prod-property").find(".on").length;//选中的项的数
            $(".prod-property li").addClass("ISSelectLi");
            if(selLength < totalLength){
                $('body,html').animate({scrollTop:220},500);
                $("#pordParameter").addClass("pordParameterSel");
                $('#pordParameter-div').show();
                $(".pord-but").hide();
                $('#sure').attr('submit',flag);
                return false;
            }
            return true;
        }

        //提示框关闭
        $('#sure').click(function(){
            selLength=$(".prod-property").find(".on").length;//选中的项的数
            if(totalLength == selLength) {
                $('#pordParameter').removeClass('pordParameterSel');
                $('#pordParameter-div').hide();
                $("#pord-buy,#pord-shoppingCart").show();
                var sub = $('#sure').attr('submit');
                $('#'+sub).click();
            } else {
                showMsg("请选择规格");
            }
        });

        function sortNumber(a,b)
        {
            return a - b
        }

        //商品数量增加 减少
        var number = 1,pord = $('.size-dd .pord-num'),total;
        $('.pord-amount-but .pord-amount-increase').on('click',function(){
            number = parseInt(pord.val());
            total = parseInt(pord.attr('total'));
            if(total >= 999) total = 999;
            if(number >= total) {
                showMsg("最多可购买"+number+"件");
                return false;
            }
            pord.val(number+1); return;
        });
        //商品数量减少
        $('.pord-amount-but .pord-amount-decrease').on('click',function(){
            number = parseInt(pord.val());
            if(number<=1) {
                showMsg("购买数量不能小于1");
                return false;
            }
            pord.val(number-1); return;
        });

        //监控数量输入
        pord.on('input',function(){
            var that = $(this);
            number = that.val();
            total = parseInt(that.attr('total'));

            if(!(/^\d+$/.test(number))) {
                showMsg("输入非法");
                that.val(1);
                return false;
            }

            if(number > 999 && total > 999){
                that.val(999);
                showMsg("最多可购买999件");
                return false;
            }

            if(number > total){
                that.val(total)
                showMsg("最多可购买" + total + "件");
                return false;
            }
            that.val(number);
        })

        //关闭未选择商品参数提示
        $(".pordParameter-close").click(function(){
            $("#pordParameter").removeClass("pordParameterSel");
            $("#pordParameter-div").hide();
            $(".pord-but").show();
            $(".pordParameter-ts").hide();
            $("#pord-Okbut").hide();
        });

