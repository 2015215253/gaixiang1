    //收藏店铺开始(登录)
     function collectionStore() {
        var name = "精东电器商城";
        var logoUrl = "";
        var storeId = "a9e20485-d2f6-4b0d-b785-8a8cbc0ca97a";
        $.ajax({
            url: "http://member.g-emall.com/collect/add-store",
            type: "GET",
            dataType: 'jsonp',
            jsonp: 'callback', //默认callback
            data: {storeID: storeId, storeName: name, pictureURL: logoUrl},
            success: function (data) {
                if(data==1){
                    layer.alert('收藏店铺成功，可在我的盖象-收藏页面查看');
                }else if(data==2){
                    layer.alert('您已收藏过此店铺。');
                }
            }
        });
        
        return 1;//标记        
    }
    //收藏店铺结束

    //异步登陆窗口
    function login() {
        var isLogin = 0;//正常登陆
        if(isLogin){//未正常登陆
            var ajaxLogin = $.cookie('login');//ajax登陆
            console.log(ajaxLogin);
            if(ajaxLogin=='true'){
                return true;
            }else{//未ajax登陆
                $.get('http://www.g-emall.com/member/user/login-ajax', {}, function (str) {
                    layer.open({
                        type: 1,
                        title: '登录盖象商城',
                        skin: 'layer-ext-gaiwang',
                        area: ['350px', '420px'],
                        content: str
                    });
                });
                return false;
            }
        }else{//正常登陆
            return true
        }
    }
    ///异步登陆窗口end

    function addCart(product,index){
        $.post(
            'http://www.g-emall.com/cart/add',
            product,
            function(data){
                layer.close(index);
//                    data = JSON.parse(data);
                if(data.status) {
                    $(".shoppingCart-float").show().stop(true, false).animate({right: 30}, 1000);
                    $.get('http://www.g-emall.com/cart/cart-count',{},function(n){
                        $('.gx-top-shopping-num,.gx-FDShopping-num').text(n);
                    });
                    setTimeout(function () {
                        $(".shoppingCart-float").hide();
                    }, 5000);

                } else {
                    if(data.msg)
                        layer.alert(
                            data.msg,{
                                icon:0,
                                title:'加入购物车'
                            });
                    else
                        layer.alert(
                            data.msg,{
                                icon:1,
                                title:'加入购物车'
                            });
                }
            },
            'json'
        );
    }
    function clearCookie(){
        var keys=document.cookie.match(/[^ =;]+(?=\=)/g);
        if (keys) {
            for (var i = keys.length; i--;)
                document.cookie=keys[i]+'=0;expires=' + new Date( 0).toUTCString()
        }
    }
    function delCookie(name) {
        var exp = new Date();
        exp.setTime(exp.getTime() - 1);
        var cval=getCookie(name);
        if(cval!=null)
            document.cookie= name + "="+cval+";expires="+exp.toGMTString();
    }
    function getCookie(name) {
        var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");
        if(arr=document.cookie.match(reg))
            return arr[2];
        else
            return null;
    }


    //登录超时，自动刷新
    setTimeout(function () {
        document.location.reload();
    },'3600'*1000);


    $(function () {
        var isDomain = false;
        if(location.href=='http://www.g-emall.com' || location.href=='http://www.g-emall.com/' || location.href=='http://www.g-emall.com/index.html' || location.href=='http://www.g-emall.com/index_tw.html'){
            isDomain = true;
        }
        $('.languageCh').click(function () {
            var languageValue = $(this).attr('language');
            $.ajax({
                type:"get",
                dataType:"jsonp",
                jsonpCallback:"language",
                url:'http://www.g-emall.com/language/set-language',
                data:{_csrf:"M1JGaF8wV1dYDSIbNFkxA30oBz8rezgBeQE/MStZHDVrOnQbER01Zw==",language:languageValue},
                success:function(data){
                    if(isDomain){
                        if(languageValue=='zh-CN'){
                            location.href = 'http://www.g-emall.com';
                        }else{
                            location.href = 'http://www.g-emall.com/index_tw.html';
                        }
                        return true;
                    }
                    location.reload();
                }

            });
        });
        setTimeout(function(){
            $.ajax({
                type:'get',
                url:'http://www.g-emall.com/site/info',
                dataType:'jsonp',
                jsonpCallback:'info',
                success:function(data){
                    $(".gx-top-shopping-num").html(data.cart);
                    $(".gx-FDShopping-num").html(data.cart);
                    $(".gx-top-welcome").html(data.html).addClass('gx-top-login');
                    if(data.a){
                        $(".storeSupport a:first").replaceWith(data.a);
                    }
                    // 在线咨询
//                    if(data.userInfo){
//                        var imUrl = "///#/?user=" + data.userInfo.gwCode + "&storeid=GW00000001&token=" + data.userInfo.imToken;
//                        $('.socket').attr('src', imUrl);
//                    }

                    if(isDomain && data.language){
                        if(data.language=='zh-TW' && location.href!='http://www.g-emall.com/index_tw.html'){
                            location.href = 'http://www.g-emall.com/index_tw.html';
                        }
                        return true;
                    }
                }
            });
        },500);
    })


        $(".webIm").click(function () {
            var imTo = $(this).attr("title");
            if(imTo.length===0) return false;
            var imFrom = $("#webImFrom").attr("title");
            if(!imFrom){
                location.href = "http://member.g-emall.com/user/login";
            }else{
                var token = "b8a01cc0a819c4bb01f2750a3f8055e5b55747a5";
                layer.open({
                    type: 2,
                    title: false,
                    move:true,
                    //closeBtn: 0, //不显示关闭按钮
                    shade: [0],
                    area: ['820px', '550px'],
                    content: ["http://img.g-emal.com/#/?user="+imFrom+"&storeid="+imTo+"&token="+token, 'no'] //iframe的url，no代表不显示滚动条
                })
            }
            return false;
        });
    

    document.domain = 'g-emall.com'; // 添加IM聊天功能跨域处理
    var pageRight = 1,maxPage,data,isGuest;
    var msgManage = true;
    /*在线咨询打开，关闭*/
    $('.gx-socket-icon').click(function(){
        $.ajax({
            type:'get',
            url:'http://www.g-emall.com/site/info',
            dataType:'jsonp',
            jsonpCallback:'info',
            success:function(data){
                if(data.userInfo){
                    var token = "fcaf94e28f0405d2a08adc0dc425a4b32a6329a0";
                    var imTo = "GW89169054";
                    var code = "GW04988760";
                    if (token == '' || code != data.userInfo.gwCode) {
                        token = data.userInfo.imToken;
                        code = data.userInfo.gwCode;
                        if (imTo != 'GW00000001')
                            imTo = 'GW00000001'
                    }

                    layer.open({
                        type: 2,
                        title: false,
                        move:true,
                        //closeBtn: 0, //不显示关闭按钮
                        shade: [0],
                        area: ['820px', '550px'],
                        content: ["http://img.g-emal.com/#/?user="+code+"&storeid="+imTo+"&token="+token, 'no'], //iframe的url，no代表不显示滚动条
                        cancel: function(){
                            msgManage = true;
                        }
                    })

                    $('#gx-socket-message').html('在线对话');
                    window.isNewMessage = 0;
                    msgManage = false;
                }else{
                    layer.alert('请先登录!');
                }
            }
        });
    })
