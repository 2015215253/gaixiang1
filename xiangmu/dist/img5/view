                    if(t.hasClass('curr')) return false; //返回
                    t.addClass('curr');
                    t.siblings().removeClass('curr');
                    hasPicture = parseInt(t.attr('has-picture'));
                    getEvaluation(hasPicture);
                })
            });

            //评论
            function getEvaluation(hasPicture) {
                var on,off,data;
                var pId = '5aaafcf4-1e20-4aba-81eb-b69a9f87beef';
                if(hasPicture){
                    on = $('#tabCon_com_2');
                    off =$('#tabCon_com_1');
                    data = {hasPicture:hasPicture,uID:pId};
                }  else {
                    on = $('#tabCon_com_1');
                    off = $('#tabCon_com_2');
                    data = {uID:pId};
                }
                $.get('http://www.g-emall.com/product/evaluation',data,function(data){
                    on.html(data.sub);
                    on.show();
                    off.hide();
                },'json');
            }

