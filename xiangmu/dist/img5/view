    //获取浏览历史
    $("#gx-click-history").one("click",function(){
        loadData(pageRight,0);
    });
    $(".gx-history-prev").click(function(){
        if(pageRight<=1){
            return false;
        }else{
            pageRight = (pageRight>1)?--pageRight:pageRight;
            loadData(pageRight,0);
        }
    });
    $(".gx-history-next").click(function(){
        if(pageRight>=maxPage){
            return false;
        }else{
            pageRight = (pageRight<maxPage)?++pageRight:pageRight;
            loadData(pageRight,0);
        }
    });
    function loadData(pageIndex,accessType){
        $.ajax({
            type: "get",
            dataType:"json",
            url: "/member/history/list",
            data:{pageIndex:pageIndex,pageSize:8,accessType:accessType,type:'right',_csrf:'M1JGaF8wV1dYDSIbNFkxA30oBz8rezgBeQE/MStZHDVrOnQbER01Zw=='},
            success: function(res){
                var html = '';
                if(res.length == 0) {
                    if(pageRight==1) {
                        $(".gx-history-no").css("display","block");
                        $(".gx-history-item").remove();
                        $(".gx-history-title").css("display","none");
                    }
                }else{
                    maxPage = res[0].PageCount;
                    isGuest = res[0].isGuest;
                    data = res.slice(1,res.length);
                    $.each(data, function (key, product) {
                        var favorite = product.favorite ? 'on' : '';
                        html += '<li>';
                        html +=     '<div class="gx-history-item-box">';
                        html +=     '<p uid="'+ product.uID +'"><i class="gx-history-item-flo index-icon ' + favorite + '"></i><i class="gx-history-item-del index-icon"></i></p>';
                        html +=     '<a href="/product/view?guid=' + product.productID + '"><img src="http://imgnew.e-gatenet.cn/' + product.pictureURL + '"></a>';
                        html +=     '</div>';
                        html +=     '<p class="gx-history-item-price">' + product.productPrice + '</p>';
                        html += '</li>';
                    });
                    $(".gx-history-item ul").html(html);
                }
            }
        });
    }
    //清空
    $(".gx-history-title span").click(function(){
        delData('all','','');
        $(".gx-history-no").css("display","block");
        $(".gx-history-item").remove();
        $(".gx-history-title").css("display","none");
    });
    //单个删除
    $(".gx-history-item ul").on("click",".gx-history-item-del",function () {
        var size = (pageRight-1)*8+$(this).closest("li").index();
        var uid = $(this).parent().attr('uid');
        $(this).closest("li").remove();
        delData('one',size,uid);
    });
    function delData(type,size,uid){
        $.ajax({
            type: "post",
            dataType: "json",
            url: "/member/history/delete",
            data: {type:type,size:size,uid:uid,_csrf:'M1JGaF8wV1dYDSIbNFkxA30oBz8rezgBeQE/MStZHDVrOnQbER01Zw=='},
            success: function(res){
                if(res==1 && type=='one'){
                    loadData(pageRight,1);
                }
            }
        });
    }
    //收藏
    $(".gx-history-item ul").delegate(".gx-history-item-flo","click",function(){
        var $this = $(this);
        if(!isGuest){
            window.location = 'http://member.g-emall.com/user/login';
        }else{
            if($this.hasClass('on')){
                layer.alert('该商品已经收藏过了');
                return false;
            }else {
                var index = $this.closest("li").index();
                var product = data[index];
                product.productPrice = product.productPrice.substr(1);
                product.pictureURL = product.pictureURL;
                $.ajax({
                    type: "GET",
                    url: "http://member.g-emall.com/collect/add-product",
                    dataType: "jsonp",
                    jsonp: "callback",
                    data: {
                        productID: product.productID,
                        productName: product.productName,
                        pictureURL: product.pictureURL,
                        price: product.productPrice,
                        categoryID: product.productCategoryID,
                        categoryName: product.productCategoryName,
                        _csrf: 'M1JGaF8wV1dYDSIbNFkxA30oBz8rezgBeQE/MStZHDVrOnQbER01Zw=='
                    },
                    success: function (data) {
                        if (data == 1) {
                            $this.addClass('on');
                            layer.alert('收藏商品成功，可在<a href="http://member.g-emall.com/collect/goods">我的盖象-收藏页面</a>查看');
                        } else if (data == 2) {
                            layer.alert('该商品已经收藏过了');
                        }else {
                            layer.alert('收藏商品失败');
                        }
                    }
                });
            }
        }

    });



        $(function () {
            $('.goodsDetails-search-shop').click(function () {
                var keywordVal = $('.keyword').val();
                //if (keywordVal === '') return false;
                window.location.href= $('.hrefValues').val() + '&keyword=' + keywordVal;
            })
            $('.goodsDetails-search-but2').click(function () {
                var keywordVal = $('.keyword').val();
                //if (keywordVal === '') return false;
                window.location.href= $('.gxsearch').val() + '&keyword=' + keywordVal;
            })
            //页面头部分
            $.post("http://www.g-emall.com/design/get-list",{'storeId':"a9e20485-d2f6-4b0d-b785-8a8cbc0ca97a",'type':1,'YII_CSRF_TOKEN': 'M1JGaF8wV1dYDSIbNFkxA30oBz8rezgBeQE/MStZHDVrOnQbER01Zw=='},
                function (data) {
                    if(!data){
                        return false;
                    }
                    var totalData = JSON.parse(data)['remark3'];
                    if(totalData){
                        var shoplogo = JSON.parse(totalData)['shoplogo'];
                        var keywords = JSON.parse(totalData)['keywords'];
                        var shopnav = JSON.parse(totalData)['shopnav'];
                        var service = JSON.parse(totalData)['service'];
                    }else {
                        shoplogo = null;
                        keywords = null;
                        shopnav = null;
                        service = null;
                    }

                    if (service){
                        var timeData = service['timeData'];
                        var phone = service['tel'];
                        var mobile = "13267018777";
                        if (service['phone'] != undefined)phone += '/'+service['phone'];
                        var worktime = timeData[0]['dayStart']+'－'+timeData[0]['dayEnd']+timeData[0]['clockStart']+'－'+timeData[0]['clockEnd'];
                        if (timeData[1] != undefined)worktime += '<br>'+timeData[1]['dayStart']+'－'+timeData[1]['dayEnd']+timeData[1]['clockStart']+'－'+timeData[1]['clockEnd'];
                        if(mobile) {
                            $('.shopInfo-float-centre').html("<p><span>联系电话：</span>" + mobile + "</p><p><span>工作时间：</span>" + worktime + "</p>");
                        }else{
                            $('.shopInfo-float-centre').html("<p><span>联系电话：</span>" + phone + "</p><p><span>工作时间：</span>" + worktime + "</p>");
                        }
                    }
                    if(shoplogo){
                        if (shoplogo['isshown'] == 'show'){
                            $('#bannerStore').attr('alt','');
                            $('#bannerStore').attr('src',shoplogo['src']).show();
                        }
                    }
                    if (keywords && keywords['list']){
                        var list = '';
                        if (keywords['isshown'] == 'show') {
                            $.each(keywords['list'], function (i) {
                                list += '<a target="_blank" href="' + keywords['list'][i]['href'] + '">' + keywords['list'][i]['text'] + '</a>';
                            })
                            $('.gx-top-search-tj').html(list);
                        }
                    }

                    if (shopnav && shopnav['list']){
                        var listNav = '';
                        if (shopnav['isshown'] == 'show'){
                            $.each(shopnav['list'],function (i) {
                                listNav += '<li><a target="_blank" href="'+shopnav['list'][i]['href']+'">'+shopnav['list'][i]['text']+'</a></li>';
                            })
                            $('#showNav').append(listNav);
                        }
                    }
                }
            )
        })
    </script>
