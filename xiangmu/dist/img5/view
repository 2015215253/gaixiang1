            /*判断商品内容滚动到所在栏目*/
            var tabCon_01, tabCon_02, tabCon_03, tabCon_04;
            $(window).scroll(function () {
                //从新计算高度
                tabCon_01=$("#tabCon-01").position().top-100;
                tabCon_02=$("#tabCon-02").position().top-100;
                tabCon_03=$("#tabCon-03").position().top-100;
                tabCon_04=$("#tabCon-04").position().top-100;
                if($(document).scrollTop()>=tabCon_01){
                    $(".gl-main-right li").removeClass("on").eq(0).addClass("on");
                }
                if($(document).scrollTop()>=tabCon_02){
                    if(!time) {
                        getEvaluation(0);
                        time = true;
                    }
                    $(".gl-main-right li").removeClass("on").eq(1).addClass("on");
                }
                if($(document).scrollTop()>=tabCon_03){
                    $(".gl-main-right li").removeClass("on").eq(2).addClass("on");
                }
                if($(document).scrollTop()>=tabCon_04){
                    $(".gl-main-right li").removeClass("on").eq(3).addClass("on");
                }
            });

            //下一张
            $(".spec-scroll .next").bind("click",function(){
                if(tempLength < countLength){
                    if((countLength - tempLength) > moveLength){
                        scrollDiv.animate({left:"-=" + moveLength + "px"}, moveTime);
                        tempLength += moveLength;
                        $(".spec-scroll .next").removeClass("on");
                        $(".spec-scroll .prev").removeClass("on");
                    }else{
                        scrollDiv.animate({left:"-=" + (countLength - tempLength) + "px"}, moveTime);
                        tempLength += (countLength - tempLength);
                        if(tempLength = countLength){
                            $(".spec-scroll .next").addClass("on");
                            $(".spec-scroll .prev").removeClass("on");
                        }
                    }
                }
            });

            //上一张
            $(".spec-scroll .prev").bind("click",function(){
                if(tempLength > 0){
                    if(tempLength > moveLength){
                        scrollDiv.animate({left: "+=" + moveLength + "px"}, moveTime);
                        tempLength -= moveLength;
                        $(".spec-scroll .prev").removeClass("on");
                        $(".spec-scroll .next").removeClass("on");
                    }else{
                        scrollDiv.animate({left: "+=" + tempLength + "px"}, moveTime);
                        tempLength = 0;
                        $(".spec-scroll .prev").addClass("on");
                        $(".spec-scroll .next").removeClass("on");
                    }
                }
            });

            //tab切换加载
            $('.pdTab2-tab li').each(function(){
